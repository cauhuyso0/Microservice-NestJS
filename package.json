{
  "name": "microservice-nest-js",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "proto:dev:user": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./proto --ts_proto_out=apps/user/src/utilities proto/user.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:dev": "npm run proto:dev:user",
    "prisma:migrate:user": "prisma migrate dev --schema libs/prisma-user/prisma/schema.prisma",
    "prisma:migrate:order": "prisma migrate dev --schema libs/prisma-order/prisma/schema.prisma",
    "prisma:migrate": "npm run prisma:migrate:user && npm run prisma:migrate:order",
    "prisma:generate:user": "prisma generate --schema libs/prisma-user/prisma/schema.prisma",
    "prisma:generate:order": "prisma generate --schema libs/prisma-order/prisma/schema.prisma",
    "prisma:generate": "npm run prisma:generate:user && npm run prisma:generate:order",
    "start:dev:notification": "nest start --watch notification",
    "start:dev:order": "nest start --watch order",
    "start:dev:payment": "nest start --watch payment",
    "start:dev:product": "nest start --watch product",
    "start:dev:user": "nest start --watch user",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "npm run prisma:generate && concurrently -c \"cyan.bold,red.bold,blue.bold,green.bold,yellow.bold\" \"npm:start:dev:*\" ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "^5.19.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "helmet": "^7.1.0",
    "moment": "^2.30.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.0.0",
    "prisma": "^5.19.1",
    "prisma-class-validator-generator": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^2.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma-user(|/.*)$": "<rootDir>/libs/prisma-user/src/$1",
      "^@app/prisma-order(|/.*)$": "<rootDir>/libs/prisma-order/src/$1",
      "^@app/ecommerce-utilities(|/.*)$": "<rootDir>/libs/ecommerce-utilities/src/$1"
    }
  }
}
