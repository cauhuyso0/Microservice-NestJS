datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["omitApi"]
  output = "../../../node_modules/@prisma/client/user"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  username String @unique
  name  String?
}
