{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "target": "ES2023",
    "paths": {
      "@apps/user": [
        "apps/user/src"
      ],
      "@apps/user/*": [
        "apps/user/src/*"
      ],
      "@apps/product": [
        "apps/product/src"
      ],
      "@apps/product/*": [
        "apps/product/src/*"
      ],
      "@apps/payment": [
        "apps/payment/src"
      ],
      "@apps/payment/*": [
        "apps/payment/src/*"
      ],
      "@apps/order": [
        "apps/order/src"
      ],
      "@apps/order/*": [
        "apps/order/src/*"
      ],
      "@apps/notification": [
        "apps/notification/src"
      ],
      "@apps/notification/*": [
        "apps/notification/src/*"
      ],
      "@lib/prisma-user": [
        "libs/prisma-user/src"
      ],
      "@lib/prisma-user/*": [
        "libs/prisma-user/src/*"
      ],
      "@lib/prisma-order": [
        "libs/prisma-order/src"
      ],
      "@lib/prisma-order/*": [
        "libs/prisma-order/src/*"
      ],
      "@lib/ecommerce-utilities": [
        "libs/ecommerce-utilities/src"
      ],
      "@lib/ecommerce-utilities/*": [
        "libs/ecommerce-utilities/src/*"
      ],
      "@libs/prisma-auth": [
        "libs/prisma-auth/src"
      ],
      "@libs/prisma-auth/*": [
        "libs/prisma-auth/src/*"
      ]
    }
  }
}